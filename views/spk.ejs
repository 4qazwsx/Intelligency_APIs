<!DOCTYPE html>                                            

<html>

  <head>

    <title><%= title %></title>

    <link rel='stylesheet' href='/stylesheets/style.css' />

    <script src="//code.jquery.com/jquery.min.js"></script>

  </head>

  <body>

    <h1>spk <%= title %></h1>

    <p>upload app</p>

    <p>result : <%= data %></p>
    <p>applist : <%= applist %></p>

    <form id='target' action='spk'>

      <select name='spkfile' size='6'>
        <option value='' selected>--select APP--</option>

	<% for (var i =0 ; i<applist.length ; i++){ %>
	<option value='<%= applist[i] %>'><%= applist[i] %></option>
	<% } %>

      </select>

      <select name='username' size='6'>
        <option value='' selected>--select user--</option>

	<% for (var i =1 ; i<userlist.length ; i++){ %>
	<option value='<%= userlist[i] %>'><%= userlist[i] %></option>
	<% } %>

      </select>

      <select name='textfile' size='6'>
	<option value='' selected>--select file--</option>

	<% for (var i =1 ; i<filelist.length ; i++){ %>
	<option value='<%= filelist[i] %>'><%= filelist[i] %></option>
	<% } %>

      </select>

      <input id='input_paramater' name='paramater' type='text' value='paramater'>

      <input type='submit' value='submit'>

    </form>

    <h1><%= data %></h1>

  </body>

</html>
